      <ComboBox fx:id="paisCombo" maxHeight="1.7976931348623157E308" prefWidth="150.0" promptText="Selecciones un país" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="8" />
      <DatePicker fx:id="nacimientoPicker" maxHeight="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" />
