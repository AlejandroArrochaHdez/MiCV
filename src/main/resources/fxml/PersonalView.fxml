      <Label text="País" GridPane.rowIndex="8" />
      <Label text="Nacionalidad" GridPane.rowIndex="9" />
      <ListView fx:id="nacionalidadListView" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="9" GridPane.rowSpan="5" />
      <Button fx:id="masButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onMasAction" text="+" GridPane.columnIndex="2" GridPane.rowIndex="9" />
      <Button fx:id="menosButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onMenosAction" text="-" GridPane.columnIndex="2" GridPane.rowIndex="10" />
